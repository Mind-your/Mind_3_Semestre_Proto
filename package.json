{
  "dependencies": {
    "fuse.js": "^7.1.0",
    "react-router-dom": "^7.8.2"
  },
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && ./mvnw spring-boot:run",
    "i:frontend": "cd frontend && npm install",
    "start:backend": "cd backend && mvnw.cmd spring-boot:run",
    "start:frontend": "npm run dev --prefix frontend",
    "dev": "concurrently --kill-others --names \"BACK,FRONT\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
